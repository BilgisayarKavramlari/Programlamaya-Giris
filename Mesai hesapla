public static void main(String[] args) {
	Scanner giris=new Scanner(System.in);
	System.out.println("Mesai saati:");
	int x=giris.nextInt();
	
	if(x>0 & x<10) System.out.println("Ücreti="+ x );
	else if(x>10 & x<20) System.out.println("Ücreti="+ x*2);
	else if(x>20 & x<40) System.out.println("Ücreti="+ x*3);
	else if(x>40 & x<100) System.out.println("Ücreti="+ x*4);
	else if(x>100) System.out.println("Ücreti="+ x*5);
	System.out.print("TL");
