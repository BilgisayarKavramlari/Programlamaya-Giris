using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace _1_den_10_a_sayma
{
    class Program
    {
       
        static void Main(string[] args)
        {
            
            for (int i = 0; i <= 10; i++)
            {
                Console.WriteLine(i + ". ");
       
            }
            int sayac1 = 0;
            do 
            {
          
                Console.WriteLine(sayac1 + ". ");
                sayac1++;
            } while (sayac1 <= 10);
            Console.ReadLine();
        }
      
        
        }
    }

