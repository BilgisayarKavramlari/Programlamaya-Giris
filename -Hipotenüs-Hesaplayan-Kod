#include <iostream>
#include <math.h>
  using namespace std;
  int main()
  {
  int a,b,c;
  cout << "Lutfen birinci dik kenari giriniz" <<endl;
  cin >> a;
  cout << "Lutfen ikinci dik kenari giriniz" <<endl;
  cin >> b;
  c =sqrt(a*a + b*b);
  cout << "Hipotenus:" << c <<endl;
  return 0;
  
  }
  
