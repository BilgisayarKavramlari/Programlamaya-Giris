#include <stdio.h>
#include <conio.h>
int main(){
int a;
printf("Lütfen puanınızı yazınız");
scanf("%d", &a);
if(a>=90 && a<=100){
printf("\nhaf notunuz: AA %d", a);
}else if(a>=80 && a<90){
printf("\nharf notunuz: BA %d", a);
}else if(a>=70 && a<80){
printf("\nharf natunuz: BB %d", a);
}else{
printf("\nhatf notunuz: F %d", a);
}
getch ();
return 0;
}
